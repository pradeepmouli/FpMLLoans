			<xsd:enumeration value="PenaltyRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the penalty rate associated with a facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="InterestCalculationMethodEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines whether agent bank is making an interest	payment based on the lender pro-rata share at the end of the period or based on the	lender position throughout the period. Agent Banks decide which way to calculate the
				interest for a deal.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="ContractPositionPeriod">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Agent bank is making an interest payment based
						on the lender's contract position throughout the period.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ProRataShare">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Agent bank is making an interest payment based
						on the lender pro-rata share.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LenderClassificationEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The following are potential lender types within a deal.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="AffiliateOfAgent">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Buyer is an affiliate of the
						agent.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AffiliateOfExistingLender">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Buyer is an affiliate of a current
						lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ApprovedFund">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Approved fund.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ExistingLender">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Buyer is a current lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LenderConsentExceptionScopeEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines the scope of the consent exception, deal or facility.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Deal"/>
			<xsd:enumeration value="Facility"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LenderConsentRequiredFromEnumOLD">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">The party that is reponsible for granting consent.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="AgentBank"/>
			<xsd:enumeration value="Borrower"/>
			<xsd:enumeration value="LetterOfCreditIssuer"/>
			<xsd:enumeration value="SwinglineBank"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LetterOfCreditPurposeEnumOLD">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">Defines the purpose of a Letter of Credit.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Documentary"/>
			<xsd:enumeration value="Financial"/>
			<xsd:enumeration value="Performance"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LetterOfCreditTypeEnumOLD">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">Defines the type of a Letter of Credit.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Commercial"/>
			<xsd:enumeration value="Documentary"/>
			<xsd:enumeration value="Performance"/>
			<xsd:enumeration value="RiskParticipated"/>
			<xsd:enumeration value="Standby"/>
			<xsd:enumeration value="Synthetic"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LienEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Facility Lien Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="First">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Second">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Third">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LoanContractRateChangeTypeEnumOLD">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">A rate factor that is	changing at the loan contract level. This has an effect on a single loan contract within the facility.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="BaseRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the actual base rate associated with a loan contract.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CapAllInRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the cap in the loan contract all-in rate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CapBaseRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the cap in the underlying index base rate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DefaultRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the default rate associated with a facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FloorAllInRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the cap in the loan contract all-in rate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FloorBaseRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the cap in the underlying index base rate.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FXRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the FX rate associated with a loan contract.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MandatoryCostRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the mandatory cost rate associated with a loan
						contract.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MarginCash">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the cash margin associated with a loan contract.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MarginPIK">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the PIK margin associated with a loan contract.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PenaltyRate">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes a change
						in the penalty rate associated with a loan contract.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LoanProductEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines a list of well-known loan (borrowing) options.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Revolver">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Revolver.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="StandByLetterOfCredit">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Stand-by Letter of Credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TermLoan">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Term Loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NonRecurringFeeTypeEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The list of one-off fee types associated with a facility.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="AmendmentFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A fee charged to the borrower for an amendment
						being made to the originally agreed credit agreement. The fee is based on a
						rate (as stated in the agreement) applied to the current commitment
						level.Calculated as a percentage of the unutilized portion of the
						facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AssignmentFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Calculated as a percentage of the unutilized
						portion of the facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BreakageFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Calculated as the cost of breaking financing on
						a loan contract which is repaid early.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FacilityExtensionFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">This fee represents any fee paid by the
						borrower to the syndicate lenders for extending an existing
						facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FundingFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Fee associated with the funding requirements
						for given facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LCIssuanceFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A flat fee paid to the Issuing Bank for a
						Letter of Credit.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UpfrontFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">This fee is also known as Participation Fee,
						Arrangement Fee etc. This fee represents compensation to the members of the
						lending syndicate (and sometimes to institutional investors as well) in
						return for their commitment of capital.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WaiverFee">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">This fee represents any fee paid by the
						borrower to the syndicate lenders / agent bank for accepting /processing
						waiver request. Waiver request is sent by the borrower to obtain approval
						from the syndicate lenders for any of their requirement, which is outside
						the terms of the agreement.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrimaryConsentEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The type of consent required for primary transactions on a given deal.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="ApproveByFundFamily">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Each 'fund family' is approved by the
						borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IndividualApproval">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Each initial lender is approved individually by
						the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PreApproveInBulk">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Entire list of initial lenders are approved in
						a single action.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RuleExceptionTypeEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines the exceptions for when a consent or rule does NOT apply, normally defined in a Credit Agreement.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="AffiliateOfAssigningLender">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Consent granted if the buyer is an affiliate of
						the assigning lender.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AffiliateOfExistingLender">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Consent granted if the buyer is an affiliate of
						an existing lender of record.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ApprovedFund">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Consent granted if the buyer is an approved
						fund of an existing lender of record.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BorrowerInDefault">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Consent granted if the borrower is in
						default.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ExistingLender">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Consent granted if the buyer is already a
						lender of record.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SellOff">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Consent granted if the party is selling off
						their entire holdings.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SeniorityEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Facility Seniority Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="JuniorSubordinate">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SeniorSecured">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SeniorSubordinate">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SeniorUnsecured">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Subordinate">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Unsecured">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TaxFormEnumOLD">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines various types of tax forms.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="W8">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Certificate of Foreign Status of Beneficial
						Owner (3 year expiration).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="W9">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">US Taxpayer Identification Form (never
						expire).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="W8BEN">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Certificate of Fireign Status of Beneficial
						Owner for US withholding.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="W8IMY">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Certificate of Foriegn Intermediary or
						Flow-Through Entity.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WEXP">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Certificate of Foriegn Government or Other
						Foreign Organzation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- **** LOAN ENUMS FOR v5.x RELEASE ****-->
	<xsd:simpleType name="AllCashEventTypeEnum">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">Defines that a payment instruction is applicable to all loan events.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="AllEvents">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">The cash is
						payable to the borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AmountAdjustmentTypeEnum">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">The type of adjustment applied to any amount.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Decrease">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes an
						decrease.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Increase">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Denotes an
						increase.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AssignmentLevelEnum">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines allocation or trade level.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Allocation">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Specific allocation level.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Trade">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Overall trade level.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ConditionsPrecedentMetEnum">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">Defines whether conditions precedent have been met in a given syndicated loan deal.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="No">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Conditions
						precedent have not been met.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Waived">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">The requirement
						for conditions precedent were waived.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Yes">
				<xsd:annotation>
					<xsd:documentation source="http://www.FpML.org" xml:lang="en">Conditions
						precedent have been met.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RateNotApplicableEnum">
		<xsd:annotation>
			<xsd:documentation source="http://www.FpML.org" xml:lang="en">Used to signify the fact that a particular rate type is no longer applicable.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="RateNotApplicable"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TradeSettlementDateEnum">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Defines trade or settlement date.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="SettlementDate">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">As of settlement date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TradeDate">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">As of trade date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
